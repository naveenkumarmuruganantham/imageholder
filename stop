[32m+[m[32m//function to print the spiral pattern[m[41m  [m
[32m+[m[32mpublic static void printPattern(int n)[m[41m  [m
[32m+[m[32m{[m[41m  [m
[32m+[m[32m//detrmines the boundary size of the array[m[41m  [m
[32m+[m[32mint size = 2 * n - 1;[m[41m  [m
[32m+[m[32m//inner loop[m[41m  [m
[32m+[m[32mfor(int i = 1; i <= size; i++)[m[41m  [m
[32m+[m[32m{[m[41m  [m
[32m+[m[32m//outer loop[m[41m      [m
[32m+[m[32mfor(int j = 1; j <= size; j++)[m[41m  [m
[32m+[m[32m{[m[41m  [m
[32m+[m[32m//calculates and prints the values for pattern[m[41m  [m
[32m+[m[32mSystem.out.print(Math.max(Math.abs(i - n), Math.abs(j - n)) + 1 + " ");[m[41m  [m
[32m+[m[32m}[m[41m  [m
[32m+[m[32mSystem.out.println();[m[41m  [m
[32m+[m[32m}[m[41m  [m
[32m+[m[32m}[m[41m  [m
[32m+[m[32m//driver code[m[41m  [m
[32m+[m[32mpublic static void main(String args[])[m[41m  [m
[32m+[m[32m{[m[41m  [m
[32m+[m[32mScanner sc = new Scanner(System.in);[m[41m  [m
[32m+[m[32mSystem.out.print("Enter the value of n: ");[m[41m  [m
